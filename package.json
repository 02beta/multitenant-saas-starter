{
  "name": "multitenant-saas-starter",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "uv run cli dev-stop && uv run cli dev dev",
    "dev:kill": "uv run cli workspace dev-stop",
    "lint": "turbo lint",
    "lint:fix": "turbo lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo clean"
  },
  "devDependencies": {
    "@workspace/eslint-config": "workspace:*",
    "@workspace/typescript-config": "workspace:*",
    "openapi-zod-client": "^1.18.3",
    "prettier": "^3.6.2",
    "turbo": "^2.5.5",
    "typescript": "5.7.3"
  },
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": ">=20"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "core-js-pure",
      "sharp"
    ]
  }
}
