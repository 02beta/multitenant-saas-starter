{
  "name": "web",
  "version": "0.1.1",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "pnpm run dev:kill && next dev --turbopack --port 3000",
    "dev:kill": "lsof -t -i:3000 | xargs -r kill -9",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "precommit": "pnpm run lint:fix && pnpm run format"
  },
  "dependencies": {
    "@radix-ui/react-slot": "^1.2.3",
    "@workspace/shared": "workspace:*",
    "@workspace/ui": "workspace:*",
    "@zodios/core": "^10.9.6",
    "framer-motion": "^12.23.12",
    "lucide-react": "^0.475.0",
    "next": "^15.4.6",
    "next-axiom": "^1.9.2",
    "next-themes": "^0.4.6",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@workspace/eslint-config": "workspace:^",
    "@workspace/typescript-config": "workspace:*",
    "typescript": "^5.9.2"
  }
}
