# Next.js Web Application Environment Variables
# Copy this file to .env.local and fill in the actual values

# Public environment variables (accessible in browser)
NEXT_PUBLIC_API_URL=http://localhost:8000
NEXT_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321
NEXT_PUBLIC_SUPABASE_PUBLIC_KEY=your_supabase_public_key_here

# Server-side only environment variables
SUPABASE_SECRET_KEY=your_supabase_secret_key_here
AUTH_JWT_SECRET=your_jwt_secret_with_at_least_32_characters

# App Configuration
NEXTAUTH_SECRET=your_nextauth_secret_here
NEXTAUTH_URL=http://localhost:3000
